{
  "name": "kitchen-helper",
  "version": "1.0.0",
  "description": "A meal planning application that helps users create personalized meal plans",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test:mealplan": "ts-node src/tests/mealPlanTest.ts",
    "test:mealplan:mock": "ts-node src/tests/mealPlanTest.ts --use-mock",
    "test:mealplan:clear-cache": "ts-node src/tests/mealPlanTest.ts --clear-cache",
    "test:api": "ts-node src/tests/testSpoonacular.ts",
    "test:env": "ts-node src/tests/testEnv.ts",
    "test:env:node": "node src/tests/testEnvNode.js",
    "test:network": "node scripts/test-network.js",
    "scrape:tasty": "cd scripts/tasty-scraper && node main.js",
    "scrape:tasty:test": "cd scripts/tasty-scraper && node main.js --test",
    "scrape:tasty:verify": "cd scripts/tasty-scraper && node run-verification-test.js",
    "scrape:tasty:stats": "cd scripts/tasty-scraper && node main.js --stats",
    "scrape:tasty:cleanup": "cd scripts/tasty-scraper && node main.js --cleanup",
    "scrape:tasty:setup": "cd scripts/tasty-scraper && node setup.js",
    "scrape:weeknight": "cd scripts/tasty-scraper && node run-weeknight-scraper.js",
    "scrape:weeknight:dry-run": "cd scripts/tasty-scraper && node run-weeknight-scraper.js --dry-run",
    "scrape:weeknight:fast": "cd scripts/tasty-scraper && node run-weeknight-scraper.js --skip-images",
    "proxy:restart": "cd proxy-server && node restart-proxy.js",
    "proxy:copy-port": "cd proxy-server && node copy-port-to-app.js",
    "proxy:setup": "npm run proxy:restart && npm run proxy:copy-port",
    "clean": "watchman watch-del-all && rm -rf ios/Pods ios/build node_modules",
    "clean:ios": "cd ios && pod deintegrate && pod cache clean --all && rm -rf Pods build",
    "clear-cache": "expo start --clear && watchman watch-del-all",
    "deep-clean": "rm -rf node_modules && npm cache clean --force && npm run clear-cache && npm install",
    "setup": "npm install && cd ios && pod install",
    "reset": "npm run clean && npm run setup",
    "debug:build": "node scripts/debug-build.js",
    "demo:mealplan": "node scripts/simpleMealPlanDemo.js",
    "test:algorithm": "node scripts/testAlgorithmSimple.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/app": "^21.12.0",
    "@react-native-firebase/auth": "^21.12.0",
    "@react-native-firebase/firestore": "^21.12.0",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "~50.0.5",
    "expo-constants": "~16.0.1",
    "expo-crypto": "~13.0.1",
    "expo-dev-client": "~3.3.8",
    "expo-file-system": "~16.0.9",
    "expo-font": "~12.0.1",
    "expo-haptics": "~13.0.1",
    "expo-linear-gradient": "~13.0.1",
    "expo-network": "~6.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-dotenv": "^3.4.9",
    "react-native-fetch-api": "^3.0.0",
    "react-native-reanimated": "^3.7.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-private-methods": "^7.25.9",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/metro-config": "^0.80.0",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "~18.2.45",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.7",
    "firebase-admin": "^12.0.0",
    "expo-modules-autolinking": "^2.1.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.41.2",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.4"
  },
  "private": true
}
